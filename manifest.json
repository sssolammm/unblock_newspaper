{
	"manifest_version": 2,
	"name": "Unblock News",
	"description": "Plugin to unblock electronic newspaper",
	"version": "0.0.1",
	"content_scripts":[
		{
		  "matches":[
			"https://www.lavanguardia.com/*"
		  ],
		  "js": ["content.js"]
		}
	  ],
	"browser_action": {
		"default_icon": "icon.png"
	  },
	"background": {
		  "scripts": ["background.js"],
		  "persistent": false
		},
	"permissions": [
	  ]
}